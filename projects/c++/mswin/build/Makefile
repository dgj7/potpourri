# Type "make help" for usage help

ifndef config
  config=debug
endif
export config

PROJECTS := mswin-test

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

mswin-test: 
	@echo "==== Building mswin-test ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f mswin-test.make

clean:
	@${MAKE} --no-print-directory -C . -f mswin-test.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   mswin-test"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
